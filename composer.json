{
  "name": "liberu-genealogy/laravel-gedcom",
  "description": "A package that converts gedcom files to Eloquent models",
  "type": "library",
  "require": {
    "php": ">=8.4",
    "liberu-genealogy/php-gedcom": "^2.0",
    "illuminate/support": "^11.0",
    "illuminate/database": "^11.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "FamilyTree365\\LaravelGedcom\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "FamilyTree365\\LaravelGedcom\\ServiceProvider"
      ]
    }
  },
  "require-dev": {
    "rector/rector": "^1.0.0",
    "driftingly/rector-laravel": "^1.2.0",
    "phpunit/phpunit": "^10.0||^11.0||^12.0",
    "laravel/framework": "^11.0"
  }
}
