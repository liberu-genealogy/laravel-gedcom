{
  "name": "liberu-genealogy/laravel-gedcom",
  "description": "A package that converts gedcom files to Eloquent models",
  "type": "library",
  "require": {
    "php": ">=8.3",
    "liberu-genealogy/php-gedcom": "^4.1.0",
    "illuminate/support": "^11.0||^12.0",
    "illuminate/database": "^11.0||^12.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "FamilyTree365\\LaravelGedcom\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "FamilyTree365\\LaravelGedcom\\ServiceProvider"
      ]
    }
  },
  "require-dev": {
    "rector/rector": "^1.0.0",
    "driftingly/rector-laravel": "^1.2.0",
    "phpunit/phpunit": "^11.0||^12.0",
    "orchestra/testbench": "^10.7",
    "mockery/mockery": "^1.6",
    "laravel/framework": "^10.0||^11.0||^12.0"
  },
  "config": {
    "use-github-api": false
  }
}
